<archetype
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0 http://maven.apache.org/xsd/archetype-1.0.0.xsd">
	<id>springboot-archetype</id>
	<allowPartial>true</allowPartial>
	<sources>
		<source>src/main/java/Application.java</source>
	</sources>
	<resources>
		<resource>src/main/resources/application.properties</resource>
	</resources>
	<testSources>
		<source>src/test/java/ApplicationTest.java</source>
	</testSources>
</archetype>
